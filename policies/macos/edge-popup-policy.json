{
  "@odata.type": "#microsoft.graph.macOSOfficeSuiteApp",
  "displayName": "Edge Popup Blocker Policy - macOS",
  "description": "Microsoft Edge popup blocking configuration for macOS devices managed by Intune",
  "bundleId": "com.microsoft.edgemac",
  "appIdentifier": "com.microsoft.edgemac",
  "settings": {
    "PopupsAllowedForUrls": [
      "https://your-allowed-domain.example"
    ],
    "PopupsBlockedForUrls": [
      "*"
    ],
    "DefaultPopupsSetting": 2,
    "AutofillAddressEnabled": false,
    "AutofillCreditCardEnabled": false,
    "PasswordManagerEnabled": false,
    "DefaultNotificationsSetting": 2,
    "TrackingPreventionLevel": 2,
    "BlockThirdPartyCookies": true,
    "SafeBrowsingEnabled": true,
    "SafeBrowsingExtendedReportingEnabled": false,
    "MetricsReportingEnabled": false,
    "DefaultSearchProviderEnabled": true,
    "SpellcheckEnabled": true,
    "TranslateEnabled": true,
    "DeveloperToolsDisabled": false,
    "BookmarkBarEnabled": true,
    "ShowHomeButton": true,
    "BrowserGuestModeEnabled": false,
    "BrowserSignin": 0,
    "SyncDisabled": false,
    "DefaultGeolocationSetting": 2,
    "DefaultMediaStreamSetting": 2,
    "DefaultWebBluetoothGuardSetting": 2,
    "DefaultWebUsbGuardSetting": 2
  }
}
